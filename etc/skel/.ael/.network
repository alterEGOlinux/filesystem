  [ -f ${HOME}/.ael/bash-ael/messages.bash ]                                  \
  && . ${HOME}/.ael/bash-ael/messages.bash

scan-net() {

    message action "Will run function <scanNet>:\n    sudo rustscan -a <ip address> -- -sV -O -sC --traceroute -Pn"
    sleep 2
    sudo rustscan -a ${1} -- -sV -O -sC --traceroute -Pn
}

