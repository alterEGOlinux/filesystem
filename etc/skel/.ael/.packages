mode        = ["ael-base"]

[xdg-user-dirs]
package     = "extra/xdg-user-dirs"
description = "Manage user directories like ~/Desktop and ~/Music"
url         = "https://www.freedesktop.org/wiki/Software/xdg-user-dirs"
mode        = ["testing"]

[xdotool]
package     = "community/xdotool"
description = "Command-line X11 automation tool"
url         = "https://www.semicomplete.com/projects/xdotool/"
man         = ["xdotool(1)"]
mode        = ["desktop", "hacker"]

[xfce4-terminal]
package     = "extra/xfce4-terminal"
description = "A modern terminal emulator primarily for the Xfce desktop environment"
url         = "https://docs.xfce.org/apps/terminal/start"
man         = ["xfce4-terminal(1)"]
mode        = ["desktop", "hacker"]

[xorg-server]
package     = "extra=/xorg-server"
description = "Xorg X server"
url         = "https://xorg.freedesktop.org"
mode        = ["desktop", "hacker"]

[xorg-server-xephyr]
package     = "extra/xorg-server-xephyr"
description = "A nested X server that runs as an X application"
url         = "https://xorg.freedesktop.org"
provides    = ["Xephyr"]
man         = ["Xephyr(1)"]
mode        = ["desktop", "hacker"]

[xorg-setxkbmap]
package     = "extra/xorg-setxkbmap"
description = "Set the keyboard using the X Keyboard Extension"
url         = "https://xorg.freedesktop.org/"
mode        = ["desktop"]

[xorg-xev]
group       = "xorg-apps"
package     = "extra/xorg-xev"
description = "Print contents of X events"
url         = "https://xorg.freedesktop.org/"
provide     = "xev"
mode        = ["desktop", "hacker"]

[xorg-xhost]
group       = "xorg-apps"
package     = "extra/xorg-xhost"
description = "Server access control program for X"
url         = "https://xorg.freedesktop.org/"
provides    = ["xhost"]
man         = ["xhost(1)"]
mode        = ["desktop", "hacker"]
note        = """
              * xhost is used to access GUI apps in docker containers.
              """

[xorg-xinit]
package     = "extra/xorg-xinit"
description = "X.Org initialisation program"
url         = "https://xorg.freedesktop.org/"
provides    = ["/usr/bin/startx",
               "/usr/bin/xinit"]
man         = ["startx(1)", "xinit(1)"]
mode        = ["desktop", "hacker"]

[xorg-xrandr]
group       = "xorg-apps"
package     = "extra/xorg-xrandr"
description = "Primitive command line interface to RandR extension"
url         = "https://xorg.freedesktop.org/"
provides    = ["/usr/bin/xrandr"]
man         = ["xrandr(1)"]
mode        = ["desktop", "hacker"]

[xterm]
package     = "extra/xterm"
description = "X Terminal Emulator"
url         = "https://invisible-island.net/xterm/"
mode        = ["desktop", "hacker"]

[youtube-dl]
package     = "community/youtube-dl"
description = "A command-line program to download videos from YouTube.com and a few more sites"
url         = "https://ytdl-org.github.io/youtube-dl/"
mode        = ["desktop", "hacker"]

[zaproxy]
package     = "community/zaproxy"
description = "Integrated penetration testing tool for finding vulnerabilities in web applications."
url         = "https://www.owasp.org/index.php/ZAP"
git         = "https://github.com/zaproxy"
mode        = ["hacker"]

[zathura]
package     = "community/zathura"
description = "Minimalistic document viewer"
url         = "https://pwmt.org/projects/zathura/"
mode        = ["desktop", "hacker"]

[zathura-pdf-mupdf]
package     = "community/zathura-pdf-mupdf"
description = "PDF support for Zathura (MuPDF backend) (Supports PDF, ePub, and OpenXPS)"
url         = "https://pwmt.org/projects/zathura-pdf-mupdf/"
mode        = ["desktop", "hacker"]

[zbar]
package     = "extra/zbar"
description = "Application and library for reading bar codes from various sources"
url         = "https://github.com/mchehab/zbar"
